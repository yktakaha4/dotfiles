[user]
	email = 20282867+yktakaha4@users.noreply.github.com
	name = Yuuki Takahashi
[alias]
	ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
        a = "add ."
        co = "commit -m"
        ch = "checkout"
        chb = "checkout -b"
        chm = "checkout master"
        chd = "!gchd() { git checkout \"$(git remote show origin | grep 'HEAD branch' | awk '{print $NF}')\"; }; gchd"
        cfg = "config --list"
        d = "diff"
        ds = "diff --staged"
        f = "fetch"
        s = "status -s"
        l = "log --oneline --graph --decorate"
        lv = "log --oneline --graph --pretty=fuller"
        ps = "push origin HEAD"
        pl = "pull origin HEAD"
        rbm = "rebase master HEAD"
[credential "https://github.com"]
	helper = !gh auth git-credential
[core]
	longpaths = true
	excludesfile = /home/tkhs/.gitignore_global
